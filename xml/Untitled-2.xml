<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    <xs:element name="personas">
        <xs:complexType>
            <xs:sequence>
                <xs:complexType>
                    <xs:attribute name="nombre" type="xs:string"></xs:attribute> 
                    <xs:attribute name="edad" type="xs:string"></xs:attribute>
                    <xs:attribute name="pais" type="xs:string"></xs:attribute>
                 </xs:complexType>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>